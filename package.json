{
  "name": "mathmesh",
  "version": "0.0.9",
  "description": "Given a latex math string, returns its positions and indices (for babylonjs)  and vertices (for threejs bufferGeometry), also gives 2D mesh for framework such as pixi",
  "types": "dist/mathmesh.d.ts",
  "files": [
    "dist/**/*"
  ],
  "main": "dist/mathmesh.js",
  "scripts": {
    "test": "npx ts-node-dev src/test/server.ts",
    "build": "rm -rf lib/ ; rm -rf dist/* ;tsc && rollup -c"
  },
  "author": "cyavictor88 <cyavictor88@gmail.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/cyavictor88/mathmesh.git"
  },
  "keywords": [
    "latex math",
    "mesh",
    "threejs",
    "babylonjs",
    "pixijs"
  ],
  "dependencies": {
    "fast-xml-parser": "^4.0.11",
    "katex": "^0.16.3",
    "mathjs": "^11.3.1"
  },
  "type": "module",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^23.0.2",
    "@rollup/plugin-json": "^5.0.1",
    "@rollup/plugin-terser": "^0.1.0",
    "@types/express": "^4.17.14",
    "@types/katex": "^0.16.1",
    "@types/node": "^18.11.9",
    "canvas": "^2.11.2",
    "express": "^4.18.2",
    "rollup": "^3.3.0",
    "rollup-plugin-dts": "^5.0.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.4"
  }
}
